/*每个item样式*/

.result-list{
    display: inline-block;
    width: 50%;
    box-sizing:border-box;
    border-bottom:solid 1rpx #ebebeb;
    text-align: center;
}
.result-list:nth-child(2n){
    margin-right: 0;
    
}
.result-list:nth-child(n+1){
 border-right: solid 1rpx #ebebeb;
}
.result-list image{
    width: 100%;
}
 .result-text{
    font-size: 32rpx;
}
.slide-image{
  width: 100%;
}
